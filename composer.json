{
    "name": "leo108/email-fake-filter-tools",
    "description": "Tools for leo108/email-fake-filter workflows",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EmailFakeFilter\\Tools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmailFakeFilter\\Tools\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "leo108",
            "email": "root@leo108.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "thecodingmachine/safe": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpstan/phpstan": "^1.10",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "thecodingmachine/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/extension-installer": "^1.3",
        "spatie/phpunit-snapshot-assertions": "^5.0",
        "laravel/pint": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "phpstan": "XDEBUG_MODE=off vendor/bin/phpstan analyse --memory-limit=4096M",
        "phpunit": "vendor/bin/phpunit",
        "pint": "vendor/bin/pint"
    }
}
